{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "whitespace": [
      true,
      "check-type",
      "check-typecast",
      "check-preblock"
    ],
    "max-line-length": [
      false
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "no-unused-expression": false,
    "only-arrow-functions": false,
    "arrow-parens": false,
    "no-bitwise": false,
    "no-console": false
  },
  "globals": {
    "process": true,
    "COUNTRY": true,
    "ENVIRONMENT": true
  },
  "rulesDirectory": [],
  "linterOptions": {
    "include": [
      "packages/*/src/**/*.ts",
      "packages/*/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/dist/*"
    ]
  }
}
